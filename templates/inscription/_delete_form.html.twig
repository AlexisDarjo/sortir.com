<form method="post" action="{{ path('app_inscription_delete', {'idParticipant': inscription.idParticipant}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ inscription.idParticipant) }}">
    <button class="m-1" type="submit">
        <span>Supprimer</span>
    </button>
</form>
